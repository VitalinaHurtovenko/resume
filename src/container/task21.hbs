<div style="margin-left: 20px;">
    {{#with heading}}
    <div class="card">
        <div class="card-body">

            {{#with main}}
            <div class="card">
                <div class="card-body">
                    <h1>{{title}}</h1>
                    <p>{{text}}</p>

                    {{#with button_text}}
                    <div>
                        <div><button class="btn btn-primary">{{solid}}</button></div>
                        <br>
                        <div><button class="btn btn-outline-primary">{{outline}}</button></div>
                    </div>
                    {{/with}}

                </div>
            </div>
            {{/with}}

            <br>

            {{#with second}}
            <div class="card">
                <div class="card-body">
                    <h2>{{title}}</h2>
                    <p>{{text}}</p>

                    {{#with button_text}}
                    <div>
                        <div><button class="btn btn-secondary">{{solid}}</button></div>
                        <br>
                        <div><button class="btn btn-outline-secondary">{{outline}}</button></div>
                    </div>
                    {{/with}}

                </div>
            </div>
            {{/with}}

            <br>

            {{#with subblock}}
            <div class="card">
                <div class="card-body">
                    <h3>{{title}}</h3>
                    <p>{{text}}</p>

                    {{#with button_text}}
                    <div>
                        <div><button class="btn btn-dark">{{solid}}</button></div>
                        <br>
                        <div><button class="btn btn-outline-dark">{{outline}}</button></div>
                    </div>
                    {{/with}}

                </div>
            </div>
            {{/with}}

        </div>
    </div>
    {{/with}}

    <br>

    {{#with nested_display}}
    <div class="card">

        <div class="card-body">
            <div class="display-1">{{title}}</div>

            <br>

            {{#with children_display}}
            <div class="card">
                <div class="card-body">
                    <div class="display-4">{{title}}</div>
                    <br>
                    <p>{{text}}</p>

                    {{#with button}}
                    <div>
                        <button class="btn btn-outline-warning">{{text}}</button>
                    </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}

            <br>

            <div>
                <h6>{{header_text}}</h6>
                <br>
                {{#with button}}
                <div>
                    <button class="btn btn-info">{{text}}</button>
                </div>
            </div>
            {{/with}}

        </div>
    </div>
    {{/with}}

    <br>

    {{#with paragraph}}
    <div class="card">
        <div class="card-body">
            <div class="text-uppercase">{{title}}</div>
            <br>
            <p>{{text}}</p>

            {{#with button}}
            <div>
                <a class="btn btn-link" href="{{url}}" target="_blank">{{text}}</a>
            </div>
            {{/with}}

            <br>

            <div class="card">
                <div class="card-body">
                    <ul>
                        {{#each list_name}}
                        <li>
                            <div>
                                <a class="btn btn-link" href="{{url}}" target="_blank">{{text}}</a>
                            </div>
                        </li>
                        {{/each}}
                    </ul>

                </div>
            </div>
        </div>
    </div>
    {{/with}}

</div>